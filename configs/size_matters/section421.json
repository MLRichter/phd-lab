{
    "model": [
        "resnet50"
    ],
    "epoch": [60],
    "batch_size": [64],

    "dataset": ["Cifar10", "MNIST"],
    "resolution": [32, 224, 1024],

    "optimizer": ["lrs60"],
    "metrics": ["Accuracy", "Top5Accuracy"],

    "logs_dir": ["../logs/"],

    "conv_method": ["channelwise"],
    "delta": [0.99],
    "data_parallel": true,
    "downsampling": 4
}