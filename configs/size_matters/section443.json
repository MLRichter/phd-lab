{
    "model": [
        "vgg11",
        "vgg13",
        "vgg16",
        "vgg19"
    ],
    "epoch": [30],
    "batch_size": [64],

    "dataset": ["MNIST"],
    "resolution": [28, 224],

    "optimizer": ["lrs"],
    "metrics": ["Accuracy", "Top5Accuracy"],

    "logs_dir": ["../logs/"],

    "conv_method": ["channelwise"],
    "delta": [0.99],
    "data_parallel": false,
    "downsampling": 4
}